when CLIENTSSL_CLIENTCERT {
    if { [SSL::cert count] > 0 } {
        set client_cert0 [string map {"\n" ""} [X509::whole [SSL::cert 0]]]
        set client_cert1 [string map {"BEGIN CERTIFICATE" ""} $client_cert0]
        set client_cert2 [string map {"END CERTIFICATE" ""} $client_cert1]
        set client_cert3 [string map {"-" ""} $client_cert2]
    }
    log "WWW ::: $client_cert3"
}

when HTTP_REQUEST {
    HTTP::header insert ssl_client_cert "$client_cert3"
}
